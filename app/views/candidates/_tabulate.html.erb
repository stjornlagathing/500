
<table class="datatable">
  <thead> 
    <tr> 
      <th>Nafn</th> 
      <th>Aldur</th> 
      <% if logged_in? %>
        <th>Val</th> 
      <% end %>
      <%# <th>Vefsíður</th>  %>
    </tr> 
  </thead> 
  <tbody>  
  <% candidates.each do |candidate| %>
    <tr>
      <td><%= link_to  candidate.name, candidate %></td>
      <td><%= candidate.years %></td>
      <% if logged_in? %>
        <td><%= show_review(candidate) %></td>
      <% end %>
      <%# 
      <td>
        <% for profile in candidate.profiles >
          < image_tag "icons/#{profile.network}.png" > 
        <% end 
      </td>
       %>
    </tr>
  <% end %>
  </tbody>
</table>
